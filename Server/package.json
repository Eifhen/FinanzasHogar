{
  "name": "home_budget",
  "version": "1.0.0",
  "description": "App para manejo de finanzas del hogar",
  "main": "src/API/startup.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "ts-node ./src/API/index.ts ",
    "dev": "npx eslint && pm2 start ecosystem.config.js --only HomeBudgetCluster --env development && pm2 logs HomeBudgetCluster",
    "prod": "pm2 start ecosystem.config.js --only HomeBudgetCluster --env production && pm2 logs HomeBudgetCluster",
    "stop": "pm2 delete ecosystem.config.js --only HomeBudgetCluster && pm2 save --force",
    "stop-dev": "pm2 stop ecosystem.config.js --only HomeBudgetCluster",
    "restart-dev": "pm2 restart ecosystem.config.js --only HomeBudgetCluster",
    "reload-dev": "pm2 reload ecosystem.config.js --only HomeBudgetCluster",
    "list": "pm2 list",
    "monit": "pm2 monit",
    "check-deps": "ncu",
    "update-deps": "ncu -u npm install"
  },
  "repository": {
    "type": "git",
    "url": "github.com/Eifhen/FinanzasHogar"
  },
  "keywords": [
    "Finanzas",
    "Hogar",
    "Home",
    "Budget"
  ],
  "author": "GJimenez",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/node": "^22.13.5",
    "eslint": "^9.21.0",
    "globals": "^16.0.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0"
  },
  "dependencies": {
    "@types/express": "^5.0.0",
    "@types/handlebars": "^4.1.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/nodemailer": "^6.4.17",
    "awilix": "^12.0.4",
    "awilix-express": "^9.0.2",
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.5.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.27.5",
    "nodemailer": "^6.10.0",
    "pm2": "^5.4.3",
    "rate-limit-redis": "^4.2.0",
    "redis": "^4.7.0",
    "tarn": "^3.0.2",
    "tedious": "^18.6.1",
    "ts-node": "^10.9.2",
    "uuid": "^11.1.0",
    "zod": "^3.24.2",
    "zod-validation-error": "^3.4.0"
  }
}
